{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "declaration": true,
    "declarationDir": "types",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "tests", "./tsup.config.ts"]
}
